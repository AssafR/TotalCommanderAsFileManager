[HKEY_CURRENT_USER\Software\Classes\Folder\shell\open\command]
@="\"C:\\Program Files\\totalcmd\\TOTALCMD64.EXE\" /O \"%1\""

[HKEY_CURRENT_USER\Software\Classes\Folder\shell\open]
